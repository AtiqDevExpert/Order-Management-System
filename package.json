{
  "name": "iamatstoreapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-d": "react-native run-ios --device \"Jugno\"",
    "ios-u": "react-native run-ios --device \"Usman iphone\"",
    "start": "react-native start",
    "set_icons": "react-native set-icon --path ./ic_full_image.png",
    "set_icons_splash": "react-native set-splash --path ./order_manager_vertical.jpg",
    "test": "jest",
    "lint": "eslint .",
    "android-clean": "rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw",
    "android-staging": "react-native run-android --variant=buildStagingRelease",
    "android-release": "react-native run-android --variant=buildProdRelease",
    "ios-release": "export PROJECT_ENV=rn && react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:ios": " react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "bundle:android": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res/",
    "postinstall": "yarn run bundle:ios",
    "android:asseble": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "android:clean": "cd android && ./gradlew clean",
    "android:releaseBuild": "cd android && ./gradlew assembleRelease",
    "ios:releaseBuild": "react-native run-ios --configuration=release",
    "android:releaseBundle": "cd android && ./gradlew bundleRelease",
    "android:release": "react-native run-android --variant=release",
    "release:build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "android:publish": "cd android && ./gradlew clean && fastlane android beta",
    "pod-install": "cd ios && pod install",
    "pod-update": "cd ios && pod update",
    "pod-update-repo": "cd ios && pod repo update",
    "setup-sentry": "npx @sentry/wizard",
    "getAllPlugins": "react-native -h",
    "link": "npx react-native link"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^6.1.2",
    "@react-native-firebase/app": "^14.8.0",
    "@react-native-firebase/messaging": "^14.8.0",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/drawer": "^6.4.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.1",
    "axios": "^0.26.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.3",
    "native-base": "^3.4.3",
    "react": "18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "0.68.1",
    "react-native-background-timer": "^2.4.1",
    "react-native-device-info": "^8.7.0",
    "react-native-element-timer": "^2.0.0",
    "react-native-event-bus": "^1.0.0",
    "react-native-floating-action": "^1.22.0",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-image-to-pdf": "^1.2.0",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-pager-view": "^5.4.15",
    "react-native-ping": "^1.2.6",
    "react-native-print": "^0.10.0",
    "react-native-reanimated": "^2.7.0",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-sound": "^0.11.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-switch": "^2.0.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-thermal-receipt-printer": "^1.2.0-rc.1",
    "react-native-view-shot": "^3.3.0",
    "react-navigation": "^4.4.4",
    "styled-components": "^5.3.5",
    "styled-system": "^5.1.5",
    "sync-storage": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@bam.tech/react-native-make": "^3.0.3",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.70.1",
    "react-test-renderer": "18.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
